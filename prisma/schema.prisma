
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id           Int      @id @default(autoincrement())
  name              String
  surname           String
  image             String
  nike_name         String   @unique
  password          String
  wishlist_products String
  wishlist_shops    String
  email             String   @unique
  status            String
  qualification     String
  updateAt          DateTime @updatedAt
  createAt          DateTime @default(now())
}

model Telephone {
  telephone_id Int      @id @default(autoincrement())
  number      Int
  dd_code     Int
  updateAt     DateTime @updatedAt
  createAt     DateTime @default(now())
}
